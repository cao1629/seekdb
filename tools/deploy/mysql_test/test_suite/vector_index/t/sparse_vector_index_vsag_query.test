--echo # 单分区查询: 全是增量数据
--disable_warnings
drop table if exists t1;
--enable_warnings

CREATE TABLE t1 (
  c1 int primary key,
  c2 sparsevector,
  vector index idx1(c2) with (lib=vsag, type=sindi, distance=inner_product)
);

